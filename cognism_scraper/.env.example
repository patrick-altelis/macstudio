# Cognism credentials (required)
COGNISM_EMAIL=your_email@example.com
COGNISM_PASSWORD=your_password

# Selenium settings
BROWSER_HEADLESS=True
BROWSER_TIMEOUT=30

# Database configuration
DATABASE_URL=sqlite:///cognism_data.db

# Scraping settings
MAX_PROFILES_PER_COMPANY=5
WAIT_BETWEEN_SEARCHES=2.0
WAIT_BETWEEN_PROFILES=1.0

# CSV Input/Output settings
INPUT_FILE=companies.csv
OUTPUT_FILE=cognism_results.csv